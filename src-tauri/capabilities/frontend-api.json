{
    "$schema": "https://schemas.tauri.app/capability.schema.json",
    "identifier": "frontend-api",
    "description": "Permissions for frontend to invoke backend commands",
    "windows": [
        "main"
    ],
    "permissions": [
        "core:app:default",
        "core:app:allow-name",
        "core:app:allow-version",
        "core:path:default",
        "core:path:allow-resolve",
        "core:path:allow-normalize"
    ]
}